<?xml version="1.0"?>
<catcher xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns="http://xlo/xsd/catcher"
		 xsi:schemaLocation="http://xlo/xsd/catcher
		 https://raw.githubusercontent.com/jiang-ruo/xlo-core/main/src/main/resources/xlo/catcher/catcher.xsd">

<!--	未登录的情况下允许访问的页面-->
	<regular>true</regular>
	
	<catch id="pass">
		<target>.*\.xb</target>
		<target>/backstage/login\.jsp</target>
		<target>/backstage/css/.*</target>
		<target>/backstage/fonts/.*</target>
		<target>/backstage/js/.*</target>
		<target>/backstage/lib/.*</target>
		<target>/backstage/imgs/.*</target>
	</catch>

<!--	已经登陆的情况下不允许访问的页面-->
	<catch id="block" regular="false">
		<target>/backstage/.eslintrc.js</target>
	</catch>
</catcher>
